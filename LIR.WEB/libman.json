{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "font-awesome@5.15.3",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "library": "datatables.net-responsive-bs@2.2.9",
      "destination": "wwwroot/lib/datatables.net-responsive-bs/",
      "files": [
        "responsive.bootstrap.min.css"
      ]
    },
    {
      "library": "datatables@1.10.21",
      "destination": "wwwroot/lib/datatables/",
      "files": [
        "js/jquery.dataTables.min.js",
        "css/dataTables.bootstrap.min.css"
      ]
    }
  ]
}